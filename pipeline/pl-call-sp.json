{
	"name": "pl-call-sp",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						}
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "PAR_Orden",
						"type": "DatasetReference",
						"parameters": {
							"directorio": "@formatDateTime(pipeline().parameters.fechaCorte.'yyyy/MM')",
							"NombreArchivo": "@pipeline().parameters.fechaCorte"
						}
					}
				]
			}
		],
		"parameters": {
			"FechaCorte": {
				"type": "string"
			}
		},
		"annotations": []
	}
}